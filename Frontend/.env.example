# API Gateway Base URL (for Vite dev server proxy target only)
# This is NOT used in browser code - all API calls use relative paths like /api/chat
VITE_API_BASE_URL=http://localhost:8080

# Neo4j Configuration
# VITE_NEO4J_PROXY_TARGET is for Vite proxy only - browser code uses /neo4j path
VITE_NEO4J_PROXY_TARGET=http://localhost:7474
VITE_NEO4J_USER=neo4j
VITE_NEO4J_PASSWORD=password

# Vite Dev Server Configuration
VITE_DEV_SERVER_PORT=5173

# Cypress Test Configuration
VITE_CYPRESS_BASE_URL=http://localhost:5173
